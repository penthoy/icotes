{
  "name": "icotes",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build && cd backend && uv run python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload --log-config logging.conf",
    "dev-frontend": "vite",
    "build": "tsc && vite build",
    "build-no-errors": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "start": "./start.sh",
    "dev-backend": "cd backend && uv run python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload --log-config logging.conf",
    "prod": "npm run build && ./start.sh",
    "logs:view": "tail -f logs/backend.log",
    "logs:clear": "rm -f logs/*.log && echo 'Logs cleared'"
  },
